Include ~/secret/.ssh/config

Host *
	IdentityFile ~/.ssh/id_ed25519
	Compression yes
	ServerAliveInterval 60
	StrictHostKeyChecking accept-new
	TCPKeepAlive yes
	HostKeyAlgorithms +ssh-rsa

Host github.com
	IPQoS throughput
	#IdentityFile ~/.ssh/id_rsa

Host github-rsa.com
	HostName github.com
	IdentityFile ~/.ssh/id_rsa
