#!/usr/bin/env fish
show trash ~/Desktop/*.png
show trash ~/Desktop/*.mov
show trash ~/Desktop/*.mp4
show rm -rf (brew --cache)
show brew services cleanup
show docker-start
show docker-clean --stop
show docker system prune --force
show docker container prune --force
show docker image prune --all --force
show docker volume prune --force
show docker network prune --force
