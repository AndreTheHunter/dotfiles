#!/usr/bin/env bash
source ~/strict.bash
asdf install nodejs $(asdf latest nodejs 12)
asdf install nodejs lts
asdf global nodejs lts
asdf reshim nodejs
npm rebuild
#TODO only install if not already installed npm list --global --depth=0 --parseable
if is-linux;then
	xargs sudo npm install --global < "$HOME/Npmfile"
else
	xargs npm install --global < "$HOME/Npmfile"
fi
asdf reshim nodejs
