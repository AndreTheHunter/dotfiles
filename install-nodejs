#!/usr/bin/env bash
#shellcheck source=strict.bash
source ~/strict.bash
for version in \
	16 \
	18 \
	20
do
	asdf install nodejs latest:$version
	asdf-uninstall-old nodejs $version
done
asdf install nodejs latest
asdf global nodejs latest
asdf-uninstall-old nodejs 21
asdf reshim nodejs
corepack enable
~/install-npm-packages
asdf reshim nodejs
