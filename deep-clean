#!/usr/bin/env fish
show cd $HOME
show rm -rf \
 .android/cache \
 .boot/cache \
 .bundle/cache \
 .cache \
 .emacs.d/.cache \
 .m2/repository \
 .node-gyp \
 .nodebrew \
 .npm/_cacache \
 .nuget/packages \
 .pub-cache \
 .serverless/cache \
 .vagrant.d \
 DryCache \
 Library/Application Support/Microsoft/Teams/Cache \
 Library/Caches/*

show docker-clean --all
for image in (docker image ls --format '{{.ID}}')
	show docker image rm --force $image
end

show find $JESI_PROJECTS -type d \( -name .shadow-cljs -or -name .cpcache -or -name .cache -or -name node_modules -or -name target \) -exec rm -rf -- {} +
