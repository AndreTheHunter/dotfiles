#!/usr/bin/env bash
#shellcheck source=strict.bash
source ~/strict.bash
for installed_version in $(brew list | ag 'openjdk@?'); do
	brew uninstall "$installed_version" || true
done
brew uninstall java java11 || true
for version in \
	graalvm-community \
	temurin \
	corretto \
	corretto-17; do
	asdf install java latest:$version
done
for version in \
	graalvm-community \
	temurin \
	corretto-17 \
	corretto-21; do
	asdf-uninstall-old java $version
done
asdf global java latest:corretto
