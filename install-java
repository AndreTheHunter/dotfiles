#!/usr/bin/env bash
source ~/strict.bash
source ~/.jabba/jabba.sh

for version in 17 18;do
	jabba uninstall amazon-corretto@$version || true
done

jabba install amazon-corretto@17=tgz+https://corretto.aws/downloads/latest/amazon-corretto-17-aarch64-macos-jdk.tar.gz
jabba install amazon-corretto@18=tgz+https://corretto.aws/downloads/latest/amazon-corretto-18-aarch64-macos-jdk.tar.gz
jabba use amazon-corretto@18
jabba alias default amazon-corretto@18

jabba ls
java -version
